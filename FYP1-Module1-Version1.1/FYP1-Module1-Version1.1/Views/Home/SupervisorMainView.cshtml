        <!-- Fixed navbar -->
        <div class="navbar navbar-size-large  navbar-default navbar-fixed-top" role="navigation">
            <div class="container-fluid">
                <div class="navbar-header">
                    <a href="#sidebar-menu" data-toggle="sidebar-menu" class="toggle pull-left visible-xs"><i class="fa fa-ellipsis-v"></i></a>
                    <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#main-nav">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>

                </div>

                <!-- Collect the nav links, forms, and other content for toggling -->
                <div class="collapse navbar-collapse" id="main-nav">
                    <ul class="nav navbar-nav">
                        <li class="dropdown">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">Profile<span class="caret"></span></a>
                            <ul class="dropdown-menu">
                                <li>@Html.ActionLink("Edit Profile", "newRegisterIdea", "Home")</li>
                                <li>@Html.ActionLink("My Ideas", "GetIdea", "Home")</li>
                               

                            </ul>
                        </li>
                        <li class="dropdown">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">Supervisor <span class="caret"></span></a>
                            <ul class="dropdown-menu">

                                <li>@Html.ActionLink("Add Idea", "newRegisterIdea", "Home")</li>
                                <li>@Html.ActionLink("Approved Ideas", "ApprovedIdeas", "Home")</li>

                            </ul>
                        </li>

                    </ul>
                    
                        <ul class="nav navbar-nav navbar-nav-bordered navbar-right">
                            <!-- notifications -->
                            <li class="dropdown notifications updates">
                                <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                    <i class="fa fa-bell-o"></i>
                                    <span class="badge badge-primary">@Model.Count</span>
                                </a>

                                <ul class="dropdown-menu" role="notification">
                                    <li class="dropdown-header">Notifications</li>
                                    @foreach (notification noti in Model)
                                    {
                                        <li class="media">

                                            <div class="media-left">
                                                <img src="/Content/images/people/50/guy-2.jpg" alt="people" class="img-circle" width="30">
                                            </div>
                                            <div class="media-body">
                                                <a href="#">@noti.notifier</a> @noti.type <a href="#">@noti.idea1.Title</a> By <a href="#">@noti.idea1.teacher.Name</a>
                                                <br />
                                                <span class="text-caption text-muted">@noti.time</span>
                                            </div>
                                            <div class="pull-right">
                                                @if (Session["Check"].ToString() == "1")
                                                {
                                                    if (noti.type.Equals(" Wants to register idea "))
                                                    {
                                                        <span class="label label-success">@Html.ActionLink("Approve", "Approve", new { id = noti.idnotification })</span>
                                                        <span class="label label-success">@Html.ActionLink("Decline", "Decline", new { id = noti.idnotification })</span>
                                                    }
                                                    else
                                                    {
                                                        <span class="label label-success">@Html.ActionLink("View", "Communication", new { id = noti.idnotification })</span>
                                                    }
                                                }
                                                else
                                                {
                                                    if (!noti.type.Equals(" Wants to register idea "))
                                                    {
                                                        <span class="label label-success">@Html.ActionLink("View", "Communication", new { id = noti.idea })</span>
                                                    }
                                                }
                                            </div>
                                        </li>
                                    }
                                </ul>

                            </li>

                            <!-- // END notifications -->
                            <!-- User -->
                            <li class="dropdown">
                                <a href="#" class="dropdown-toggle user" data-toggle="dropdown">
                                    <img src="~/Content/images/people/110/guy-5.jpg" alt="Bill" class="img-circle" width="40" /> @Session["Name"].ToString() <span class="caret"></span>
                                </a>
                                <ul class="dropdown-menu" role="menu">

                                    <li>@Html.ActionLink("Logout", "MainView", "Home")</li>
                                </ul>
                            </li>
                        </ul>
                        
                    </div>
                <!-- /.navbar-collapse -->

            </div>
