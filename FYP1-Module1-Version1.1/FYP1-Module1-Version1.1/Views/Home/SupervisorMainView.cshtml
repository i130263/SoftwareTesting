                        <ul id="course-menu">
                            <li><span>@Html.ActionLink("Add Idea", "newRegisterIdea", "Home")</span></li>
                            <li>@Html.ActionLink("My Ideas", "MyIdeas", "Home")</li>

                        </ul>
                    </li>
                    <li class="hasSubmenu">
